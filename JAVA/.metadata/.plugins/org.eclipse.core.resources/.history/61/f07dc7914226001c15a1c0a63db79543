package fundamentos;

public class Temperatura {
	
	public static void main(String[] args) {
		// (°F - 32) * 5/9 = C°
		final double AJUSTE = 32.0;
		final double FATOR =  5 / 9.0;
		double f = 86;
		
		double total = (f - AJUSTE) * FATOR;
		System.out.println("Temperatura: " + f + "°F || " + total +"°C");		
	}
}
